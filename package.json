{
  "name": "@jasonpollman/memoizor",
  "version": "1.0.0",
  "description": "Awesomely simple memoize library with distributed capabilities.",
  "license": "ISC",
  "main": "dist/",
  "private": true,
  "author": {
    "name": "Jason James Pollman",
    "email": "jason.j.pollman@gmail.com"
  },
  "keywords": [
    "memoize",
    "memoizee",
    "cache",
    "function",
    "store",
    "utility"
  ],
  "homepage": "https://github.com/JasonPollman/Memoizor",
  "repository": {
    "type": "git",
    "url": "https://github.com/JasonPollman/Memoizor"
  },
  "bugs": {
    "url": "https://github.com/JasonPollman/Memoizor/issues",
    "email": "jason.j.pollman@gmail.com"
  },
  "scripts": {
    "compile": "NODE_ENV=production ./node_modules/.bin/babel src -d dist",
    "lint": "./node_modules/.bin/eslint ./src",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha ./test/unit/**",
    "coverage": "NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/mocha ./test/unit/**",
    "prepublish": "npm run compile"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-source-map-support-for-6": "0.0.5",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-promise-to-bluebird": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.9.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.1.2"
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "bluebird": "^3.4.7",
    "debug": "^2.6.0",
    "json-normalize": "^1.1.1",
    "lodash": "^4.17.4"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**.js"
    ],
    "exclude": [
      "src/examples"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
